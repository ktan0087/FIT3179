{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 650,
  "height": 450,
  "title": "Deaths by risk factors, 2019",
  "data": {
    "url": "https://raw.githubusercontent.com/ktan0087/FIT3179/main/DV2/data/Death%20by%20factor.csv"
  },
  "mark": {
    "type": "bar"
  },
  "encoding": {
    "x": {
      "field": "Max Deaths",
      "type": "quantitative",
      "axis": false
    },
    "y": { "field": "Factors", "type": "nominal", "sort": "-x", "title": "" },
    "tooltip": [
      { "field": "Factors", "type": "nominal" },
      { "field": "Max Deaths", "type": "quantitative" }
    ]
  },
  "config": {
    "view": {
      "stroke": "transparent"
    },
    "background": "transparent"
  }
}
