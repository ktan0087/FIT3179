{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/ktan0087/FIT3179/main/DV2/data/PM2.5_mean_annual_exposure_by_GDP.csv"
  },
  "mark": "line",
  "width": { "step": 50 },
  "encoding": {
    "x": {
      "field": "Year",
      "type": "nominal",
      "scale": { "padding": 0.5 }
    },
    "y": {
      "field": "PM",
      "type": "quantitative"
    },
    "color": { "field": "Region", "type": "nominal" }
  }
}
