{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/ktan0087/FIT3179/main/DV2/data/PM2.5_mean_annual_exposure.csv"
  },
  "width": 800,
  "height": 500,
  "params": [
    {
      "name": "year_selection",
      "value": 2010,
      "bind": {
        "input": "range",
        "min": 2010,
        "max": 2019,
        "step": 1,
        "name": "Year: "
      }
    }
  ],
  "transform": [{ "filter": "datum.Year == year_selection" }],
  "encoding": {
    "x": {}
  }
}
